<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.design.widget.AppBarLayout
        android:id="@+id/toolbar_edit_profile_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar_edit_profile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/red"
            android:elevation="7dp"
            android:minHeight="?attr/actionBarSize"
            android:theme="@style/ThemeOverlay.AppCompat.Dark"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:titleTextColor="@android:color/white">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start">

                <TextView
                    android:id="@+id/toolbar_tv_edit_profile"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:gravity="center_vertical"
                    android:text="@string/toolbar_text_edit_profile_detail"
                    android:textColor="@color/white"
                    android:textSize="15sp" />

            </RelativeLayout>

        </android.support.v7.widget.Toolbar>
    </android.support.design.widget.AppBarLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/toolbar_edit_profile_bar">


        <RelativeLayout
            android:id="@+id/rl_main_edit_profile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/rl_edit_profile_center_image"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/edit_screen">


                <!--<RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="230dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ic_demo_bg_image" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/edit_background_black_transparent" />

                </RelativeLayout>-->

                <RelativeLayout
                    android:id="@+id/rl_edit_profile_imageview"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/civ_edit_profile_image"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:background="@drawable/ic_profile_image_bg"
                        android:src="@drawable/ic_the_app_guru"
                        app:civ_border_color="#FFFFFFFF"
                        app:civ_border_width="2dp" />


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_edit_profile_btn"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="-40dp"
                    android:layout_marginStart="-40dp"
                    android:layout_toEndOf="@id/rl_edit_profile_imageview"
                    android:layout_toRightOf="@id/rl_edit_profile_imageview">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/ic_picker_imagee" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_merchant_detail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rl_edit_profile_imageview"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:paddingBottom="5dp">

                    <TextView
                        android:id="@+id/tv_edit_merchant_profile_name"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_alignParentTop="true"
                        android:maxLines="1"
                        android:padding="2dp"
                        android:singleLine="true"
                        android:text="@string/merchant_name"
                        android:textAlignment="center"
                        android:textColor="#FFFFFFFF"
                        android:textSize="15sp"
                        android:textStyle="normal" />


                    <TextView
                        android:id="@+id/tv_edit_merchant_email"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_below="@id/tv_edit_merchant_profile_name"
                        android:maxLines="1"
                        android:padding="2dp"
                        android:singleLine="true"
                        android:text="@string/merchant_email"
                        android:textAlignment="center"
                        android:textColor="#FFFFFFFF"
                        android:textSize="15sp"
                        android:textStyle="normal" />


                    <TextView
                        android:id="@+id/tv_edit_merchant_mobile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_below="@id/tv_edit_merchant_email"
                        android:maxLines="1"
                        android:padding="2dp"
                        android:singleLine="true"
                        android:text="@string/merchant_mobile"
                        android:textAlignment="center"
                        android:textColor="#FFFFFFFF"
                        android:textSize="15sp"
                        android:textStyle="normal" />


                    <TextView
                        android:id="@+id/tv_edit_merchant_working_place"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_below="@id/tv_edit_merchant_mobile"
                        android:maxLines="1"
                        android:padding="2dp"
                        android:singleLine="true"
                        android:text="@string/merchant_working_place"
                        android:textAlignment="center"
                        android:textColor="#FFFFFFFF"
                        android:textSize="15sp"
                        android:textStyle="normal" />

                </RelativeLayout>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rl_edit_profile_fields"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/rl_edit_profile_center_image"
                android:padding="20dp">

                <TextView
                    android:id="@+id/tv_edit_profile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="@string/text_edit_profile"
                    android:textColor="@color/red"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <android.support.v7.widget.CardView
                    android:id="@+id/cv_et_edit_profile_change_password"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_edit_profile"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="3dp"
                    app:cardMaxElevation="3dp">

                    <RelativeLayout
                        android:id="@+id/rl_et_edit_profile_username"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/btn_presse_effect_upload"
                        android:padding="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/textview_change_password_click"
                            android:textColor="@color/whitee"
                            android:textSize="15sp" />

                    </RelativeLayout>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/cv_et_edit_profile_phone_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/cv_et_edit_profile_change_password"
                    android:layout_marginTop="15dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:visibility="gone"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="3dp"
                    app:cardMaxElevation="3dp">

                    <RelativeLayout
                        android:id="@+id/rl_et_edit_profile_phone_number"
                        android:layout_width="50dp"
                        android:layout_height="60dp"
                        android:layout_centerVertical="true"
                        android:paddingBottom="15dp"
                        android:paddingEnd="15dp"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp"
                        android:paddingStart="15dp"
                        android:paddingTop="25dp">

                        <ImageView
                            android:id="@+id/iv_et_edit_profile_phone_number"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/ic_phone_number"
                            android:contentDescription="@string/login_email_user" />

                    </RelativeLayout>

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/tin_et_edit_profile_phone_number"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="50dp"
                        android:layout_marginStart="50dp"
                        android:textColorHint="@color/grey">

                        <EditText
                            android:id="@+id/edt_et_edit_profile_phone_number"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:ellipsize="start"
                            android:gravity="clip_horizontal"
                            android:hint="@string/phone_number"
                            android:inputType="phone"
                            android:maxLines="1"
                            android:textColor="@color/black"
                            android:textColorHint="@color/grey"
                            android:textSize="15sp" />

                    </android.support.design.widget.TextInputLayout>

                </android.support.v7.widget.CardView>


                <RelativeLayout
                    android:id="@+id/rl_main_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/cv_et_edit_profile_phone_number"
                    android:layout_marginTop="20dp"
                    android:visibility="gone">


                    <TextView
                        android:id="@+id/tv_seller_type"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Select Service City"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_main_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rl_main_city"
                    android:background="@color/white"
                    android:visibility="gone">

                    <com.jaredrummler.materialspinner.MaterialSpinner
                        android:id="@+id/mtspn_places"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="@string/merchant_service_place_hint"
                        android:textSize="15sp"
                        app:ms_arrow_tint="@color/black"
                        app:ms_dropdown_height="wrap_content"
                        app:ms_dropdown_max_height="300dp"
                        app:ms_hide_arrow="false"
                        app:ms_text_color="@color/black" />


                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/rl_sub_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rl_main_spinner"
                    android:layout_marginTop="20dp"
                    android:visibility="gone">


                    <TextView
                        android:id="@+id/tv_area"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:text="Select Service Area"
                        android:textColor="@color/black"
                        android:textSize="12sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_sub_city_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rl_sub_city"
                    android:background="@color/white"
                    android:visibility="gone">

                    <com.jaredrummler.materialspinner.MaterialSpinner
                        android:id="@+id/mtspn_area"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="@string/merchant_service_area_hint"
                        android:textSize="15sp"
                        app:ms_arrow_tint="@color/black"
                        app:ms_dropdown_height="wrap_content"
                        app:ms_dropdown_max_height="300dp"
                        app:ms_hide_arrow="false"
                        app:ms_text_color="@color/black" />


                </RelativeLayout>


                <android.support.v7.widget.CardView
                    android:id="@+id/cv_et_edit_profile_pan_card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rl_sub_city_spinner"
