                    android:foreground="?android:attr/selectableItemBackground"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="3dp"
                    app:cardMaxElevation="3dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp">

                        <TextView
                            android:id="@+id/tv_text_pan_card"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="Upload Your Pan Card"
                            android:textColor="@color/black"
                            android:textSize="18sp" />

                        <ImageView
                            android:id="@+id/iv_pan_card"
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:layout_below="@id/tv_text_pan_card"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="5dp"
                            android:visibility="gone" />

                        <ProgressBar
                            android:id="@+id/pb_pan_card"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_below="@id/tv_text_pan_card"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="25dp"
                            android:visibility="gone" />


                        <!--<Button-->
                        <!--android:id="@+id/tv_browse_pan_card"-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="40dp"-->
                        <!--android:layout_below="@id/iv_pan_card"-->
